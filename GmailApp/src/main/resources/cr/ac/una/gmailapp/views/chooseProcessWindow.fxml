<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="contenedor" stylesheets="@../styles/notificationprocessmaintenance.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.gmailapp.controllers.ChooseProcessController">
   <center>
      <AnchorPane id="AnchorPane" prefHeight="298.0" prefWidth="516.0">
         <children>
            <VBox alignment="CENTER" layoutX="13.0" layoutY="14.0" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="14.0">
               <children>
                  <TextField fx:id="textField" prefHeight="25.0" prefWidth="491.0" promptText="Search --click twice to select">
                     <font>
                        <Font name="Yu Gothic UI Semibold" size="13.0" />
                     </font>
                  </TextField>
                  <TableView fx:id="tableView" prefHeight="243.0" prefWidth="491.0">
                    <columns>
                      <TableColumn fx:id="notCol" prefWidth="75.0" text="NotificationProcess" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </center>
   <top>
      <HBox fx:id="header" alignment="CENTER" prefHeight="67.0" prefWidth="516.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" prefHeight="21.0" prefWidth="452.0" text="Select a notification Process">
               <font>
                  <Font name="Yu Gothic UI Semibold" size="15.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </top>
</BorderPane>
